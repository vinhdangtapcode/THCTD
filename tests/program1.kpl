Program Program1;  

Var a : Integer;
    b : Integer;
    sum : Integer;
    diff : Integer;
    prod : Integer;

Begin
    a := ReadI;
    b := ReadI;
    
    sum := a + b;
    
    diff := a - b;
    
    prod := a * b;
    
    Call WriteLn;
    Call WriteI(sum);
    Call WriteLn;
    Call WriteI(diff);
    Call WriteLn;
    Call WriteI(prod);
    Call WriteLn;
End.
